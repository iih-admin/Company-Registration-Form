<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  

  <ItemGroup>
    <PackageReference Include="ClosedXML" Version="0.105.0" />
    <PackageReference Include="Google.Apis.Sheets.v4" Version="1.70.0.3819" />
  </ItemGroup>
<ItemGroup>
  <!-- make sure no Dockerfile is treated as source/content -->
  <Compile Remove="Dockerfile" />
  <None Remove="Dockerfile" />
  <Content Remove="Dockerfile" />
  <!-- also guard against any nested copies -->
  <Compile Remove="**/Dockerfile" />
  <None Remove="**/Dockerfile" />
  <Content Remove="**/Dockerfile" />
</ItemGroup>
</Project>
